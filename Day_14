def fibovacci_seq(n):     
    fs1=0
    fs2 =1
    if n<0:
        return 0
    elif n==1:
        print (1)
    elif n>1:
        print(fs1,'', end='')
        print(fs2,'',end='')
        for x in range(0,n):
            next = fs1 + fs2
            fs1= fs2
            fs2=next
            print(next,'',end='')
       
fibovacci_seq(7)
