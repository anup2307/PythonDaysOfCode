import unittest

def add_num(a,b):
    return a+b
    
class TestPrime(unittest.TestCase):
    def test_addition(self):
        self.assertEqual(add_num(2,3),5)
        self.assertEqual(add_num(5,6),11)
        self.assertEqual(add_num(9,0),10)
        self.assertEqual(add_num(11,1),12)

if __name__=='__main__':
	unittest.main()
